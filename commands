//Create App
yarn create react-app expensis-web --template typescript

//React Router
yarn add react-router-dom
yarn add -D @types/react-router-dom
yarn add -D @types/react-router

//GIT
git init
getJwtToken
git commit -m "Initializing Repo"
git branch -M main
git remote add origin https://github.com/adolfredo-coneo/Expensis-web.git
git push -u origin main

//Firebase 
yarn add firebase

//VS code
TypeScript: Restart TS server

//React Redux
yarn add react-redux
yarn add @reduxjs/toolkit

//Storybook
npx sb init

//fix babel loader error
delete yarn.lock
delete node_modules
yarn
"resolutions": {
    "babel-loader": "8.1.0"
  }
  yarn install

//Material UI
yarn add @material-ui/core
yarn add -D storybook-addon-material-ui

//Formik
yarn add formik

//eslint
yarn add -D eslint
yarn run eslint --init